@model MySchool.Entities.AdmissionFormEntities

@{
    ViewBag.Title = "Index";
}

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Scripts/Admission.js"></script>

<h3>
    List for Admission 
</h3>
<body>
    @Html.Hidden("getadmissionformpagewise", @Url.Action("GetAdmissionFormPageWise", "Admission"))

    
    <div class="container-fluid">
        <!-- START DATATABLE 1 -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-body">

                        <div class="table-responsive">
                            <table id="tblStudentListForAdmission" class="table table-striped table-hover">
                                <thead>
                                    <tr>
                                        <th>Form Number</th>
                                        <th>Student</th>
                                        <th>Father Name</th>
                                        <th>Mother Name</th>
                                        <th>Admit</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END DATATABLE 1 -->
    </div>
</body>

@section Styles {
    @Styles.Render("~/bundles/datatablesCss")
    @Styles.Render("~/bundles/SweetAlertCss")
}
@section Scripts {
    @Scripts.Render("~/bundles/datatables")
    @Scripts.Render("~/bundles/demoDatatable")
    @Scripts.Render("~/bundles/parsley")
    @Scripts.Render("~/bundles/SweetAlert")
}

<script type="text/javascript">
    $(document).ready(function () {
        SetUpGrid();
    });
</script>
